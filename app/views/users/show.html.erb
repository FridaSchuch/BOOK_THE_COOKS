
<div id="body">
  <div id="header">
    <h1><%= @user.username %></h1>
  </div>
  <div id="bookings">
    <div id="my-bookings">
      <% @my_booking.each do |booking| %>
        <div class="card" style="width: 18rem;">
          <div class="card-body">
            <h5 class="card-title"><%= booking.name %></h5>
            <p class="card-text"><%= booking.date %></p>
          </div>
        </div>
      <% end %>
    </div>
    <div id="booked">

    </div>
  </div>
</div>
